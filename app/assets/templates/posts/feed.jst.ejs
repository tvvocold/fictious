<h1>This is other backbone</h1>
<ul>
  <% posts.each(function(post) { %>
    <% var author_id = post.get('author_id') %>
    <% var user = FictiousApp.users.get(author_id) %>
    <% var profilePic = user.get('profile_picture_url_thumb') %>
    <li class="post-list">
      <a href="#posts/<%= post.get('id') %>">
        <h3><%= post.get('title') %></h3>
        <h4><%= post.get('subtitle') %></h4>
        <img src="<%= profilePic %>">
        <h5><%= user.get('username') %></h5>
      </a>
    </li>
  <% }) %>
</ul>
