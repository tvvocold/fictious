<ul>
  <% posts.each(function(post) { %>
    <% var authorId = post.get('author_id') %>
    <% var author = FictiousApp.users.get(authorId) %>
    <li class="post-list">
      <a href="/posts/<%= post.get('id') %>">
        <h3><%= post.get('title') %></h3>
        <h4><%= post.get('subtitle') %></h4>
        <h5><%= author.get('username')  %></h5>
      </a>
    </li>
  <% }) %>
</ul>
