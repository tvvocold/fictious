

<%= form_for @user do |f| %>
  <%= f.label :profile_picture %>
  <%= f.file_field :profile_picture %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
